constraint CN_PT_NB_1YWAXGZ_C {
  description "Restrictions for Dimensions"
  status Released
  source {
    objects: NOTEBOOK is_a (300)PT_NB . 
    restrictions:
      NOTEBOOK.SCREEN_SIZE = '10.1' if NOTEBOOK.NOTEBOOK_TYPE = 'netbook',
      NOTEBOOK.SCREEN_SIZE in ('15.4', '17') if NOTEBOOK.NOTEBOOK_TYPE =
        'multimedia',
      NOTEBOOK.SCREEN_SIZE in ('12', '14', '15.4', '17') if
        NOTEBOOK.NOTEBOOK_TYPE in ('development', 'business'). 
    inferences: NOTEBOOK.SCREEN_SIZE. 
  }
}

constraint CN_PT_NB_U7GTMF_C {
  description "Restrictions for Cores"
  status Released
  source {
    objects: NOTEBOOK is_a (300)PT_NB . 
    restrictions:
      NOTEBOOK.CORE_AMOUNT in (2,  4) if
        NOTEBOOK.NOTEBOOK_TYPE in ('development', 'multimedia', 'business'),
      NOTEBOOK.CORE_AMOUNT in (1,  2) if NOTEBOOK.NOTEBOOK_TYPE =
        'netbook'. 
    inferences: NOTEBOOK.CORE_AMOUNT. 
  }
}

constraint CN_PT_NB_1U0S4R2_C {
  description "Restrictions for HDD"
  status Released
  source {
    objects: NOTEBOOK is_a (300)PT_NB . 
    restrictions:
      NOTEBOOK.HARD_DISK_SIZE in ('60', '120') if NOTEBOOK.NOTEBOOK_TYPE =
        'netbook',
      NOTEBOOK.HARD_DISK_SIZE in ('120', '340') if
        NOTEBOOK.NOTEBOOK_TYPE in ('development', 'multimedia', 'business').
    
    inferences: NOTEBOOK.HARD_DISK_SIZE. 
  }
}

constraint CN_PT_NB_SOZCLS_C {
  description "Restrictions for Memory"
  status Released
  source {
    objects: NOTEBOOK is_a (300)PT_NB . 
    restrictions:
      NOTEBOOK.RAM_MODULES_AMOUNT in (2,  3,  4) if
        NOTEBOOK.NOTEBOOK_TYPE in ('netbook'),
      NOTEBOOK.RAM_MODULES_AMOUNT in (3,  4,  5,  6) if
        NOTEBOOK.NOTEBOOK_TYPE in ('business', 'development'),
      NOTEBOOK.RAM_MODULES_AMOUNT in (5,  6,  7,  8) if
        NOTEBOOK.NOTEBOOK_TYPE in ('multimedia'). 
    inferences: NOTEBOOK.RAM_MODULES_AMOUNT. 
  }
}

