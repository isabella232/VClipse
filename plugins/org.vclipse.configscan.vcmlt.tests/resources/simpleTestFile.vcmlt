import "ABC.vcml"

testgroup 'ExampleTG' "Example Test Group" success {
	CORE = 'NOCORE' @ /
	CORE = 'NOCORE' @ / (0010) MAT1
	CORE = 'NOCORE' @ / (0010) MAT1 / (0020) MAT2
	CORE = 'NOCORE' @ / (0010) MAT1 / (0020) MAT2 / (0030) MAT3
	CORE = 'NOCORE' @ / MAT1
	CORE = 'NOCORE' @ / MAT1 / MAT2
	CORE = 'NOCORE' @ / MAT1 / MAT2 / (0030) MAT3 / (0040) MAT4
	check TEMPVAR hidden disabled unset @ /
	check POWERSUPPLY enabled visible set = 'DC' @ /
	check CORE = '013' @ /
	checkDomain APPLICATION {
		'ADM-1'
		'ADM-2'
		not 'ADM-3'
		not 'ADM-4'
		} @ /
	checkDomainStrict APPLICATION {
		'ADM-1'
		'ADM-2'
		} @ /
	check quantity = 1 @ / MAT1
	check countitems = 5 @ /
	check consistent @ /
	check complete @ /
}