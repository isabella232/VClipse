constraint TEST_NCRLHS_TABLEFUNCTION {
  description "validator test"
  status Released
  source { 
    objects: OBJECT is_a (300)TEST_CLASS . 
    restrictions:
    	not table TEST_TABLE(TEST_CHARACTERISTIC_ONE = OBJECT.TEST_CHARACTERISTIC_TWO),
    	not function TEST_FUNCTION(TEST_CHARACTERISTIC_FIVE = OBJECT.TEST_CHARACTERISTIC_FOUR)
    inferences: 
    	OBJECT.TEST_CHARACTERISTIC_ONE, 
    	OBJECT.TEST_CHARACTERISTIC_TWO,
    	OBJECT.TEST_CHARACTERISTIC_THREE, 
    	OBJECT.TEST_CHARACTERISTIC_FOUR,
    	OBJECT.TEST_CHARACTERISTIC_FIVE, 
		OBJECT.TEST_CHARACTERISTIC_SIX. 
  } 
} 
 
varianttable TEST_TABLE {
	description "description"
	status released
	group "group"
	arguments {
		key TEST_CHARACTERISTIC_ONE
		key TEST_CHARACTERISTIC_TWO
	}
}

variantfunction TEST_FUNCTION {
	description "description"
	status released
	group "group"
	arguments {
		in TEST_CHARACTERISTIC_FIVE
	}
}

characteristic TEST_CHARACTERISTIC_ONE
characteristic TEST_CHARACTERISTIC_TWO
characteristic TEST_CHARACTERISTIC_THREE 
characteristic TEST_CHARACTERISTIC_FOUR
characteristic TEST_CHARACTERISTIC_FIVE
characteristic TEST_CHARACTERISTIC_SIX
class (300)TEST_CLASS


